{
  "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]],
  "plugins": [
    ["styled-components"],
    ["@babel/plugin-syntax-jsx"],
    ["module-resolver", { "root": ["./"] }],
    "inline-react-svg"
  ],

  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": ["transform-export-extensions"],
      "only": ["./**/*.js", "node_modules/jest-runtime"]
    }
  }
}
